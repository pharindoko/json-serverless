{
  "name": "json-server-less-lambda",
  "version": "1.0.0",
  "description": "a lambda solution for json-server",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=local  nodemon --exec babel-node src/server.js --inspect",
    "build": "babel src -d dist -D",
    "deploy": "npm run build && serverless deploy --stage=dev"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8.10"
          }
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pharindoko/ngx-chartboard-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pharindoko/ngx-chartboard-backend/issues"
  },
  "homepage": "https://github.com/pharindoko/ngx-chartboard-backend#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "dotenv": "^8.0.0",
    "json-server": "^0.14.2",
    "lowdb": "^1.0.0",
    "lowdb-adapter-aws-s3": "^1.1.2",
    "pino": "^5.10.6",
    "serverless-http": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.2.2",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "nodemon": "^1.19.0",
    "pino-pretty": "3.0.0"
  }
}
